{
  "multipart": [
    {
      "when": { "connected_side": "north" },
      "apply":{"model": "compactmachines:tunnels/north_layer" }  
    },
    {
      "when": { "connected_side": "east" },
      "apply": {"model": "compactmachines:tunnels/east_layer"} 
    },
    {
      "when": { "connected_side": "south" },
      "apply": { "model": "compactmachines:tunnels/south_layer" }
    },
    {
      "when": { "connected_side": "west"},
      "apply" : { "model" : "compactmachines:tunnels/west_layer" }
    },
    {
      "when": { "connected_side": "up" },
      "apply": { "model" : "compactmachines:tunnels/up_layer" }
    },
    {
      "when": { "connected_side" : "down" },
      "apply": { "model" : "compactmachines:tunnels/down_layer"}
    },
    {
      "when" : { "connected_side": "none" },
      "apply": { "model" : "compactmachines:tunnels/none_layer" }
    },
    {
      "when": { "going": "incoming" },
      "apply": { "model": "compactmachines:tunnels/incoming" }
    },
    {
      "when" : { "going": "outgoing" },
      "apply" : { "model": "compactmachines:tunnels/outgoing" }
    },
    {
      "when" : { "going": "neither" },
      "apply" : { "model" : "compactmachines:tunnels/neither" }
    },
    {
      "when": { "ttype": "redstone" },
      "apply" : { "model" : "compactmachines:tunnels/redstone_tunnel" }
    },
    {
      "when" : { "ttype": "normal" },
      "apply" : { "model" : "compactmachines:tunnels/item_tunnel" }
    }
  ]
}
